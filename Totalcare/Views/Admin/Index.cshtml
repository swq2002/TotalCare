@model AdminViewModel

@{
    Layout = "_AdminLayout";
}

<div class="col-lg-4">
    <div class="white_card card_height_100 default_border_1px mb_20">
        <div class="white_card_header">
            <div class="box_header m-0">
                <div class="main-title">
                    <h3 class="m-0">Number Of Subsription</h3>
                </div>

            </div>
        </div>
        <div class="white_card_body">
            <center>
                <h2 class="text-black centerd">@Model.Users.Count</h2>

            </center>
            <span class="float-right display-5 opacity-5"> <svg xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 384 512"><path d="M0 448c0 35.3 28.7 64 64 64H224V384c0-17.7 14.3-32 32-32H384V64c0-35.3-28.7-64-64-64H64C28.7 0 0 28.7 0 64V448zM171.3 75.3l-96 96c-6.2 6.2-16.4 6.2-22.6 0s-6.2-16.4 0-22.6l96-96c6.2-6.2 16.4-6.2 22.6 0s6.2 16.4 0 22.6zm96 32l-160 160c-6.2 6.2-16.4 6.2-22.6 0s-6.2-16.4 0-22.6l160-160c6.2-6.2 16.4-6.2 22.6 0s6.2 16.4 0 22.6zM384 384H256V512L384 384z" /></svg></span>
        </div>
    </div>
</div>
@{
    var totalProfit = ViewBag.Profit1 + ViewBag.Profit2;
}
<div class="col-lg-4">
    <div class="white_card card_height_100 default_border_1px mb_20">
        <div class="white_card_header">
            <div class="box_header m-0">
                <div class="main-title">
                    <h3 class="m-0">Net Profit</h3>
                </div>

            </div>
        </div>
        <div class="white_card_body">
            <center>
                <h2 class="text-black centerd">$@totalProfit</h2>

            </center>
            <span class="float-right display-5 opacity-5"><svg xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 576 512"><path d="M64 64C28.7 64 0 92.7 0 128V384c0 35.3 28.7 64 64 64H512c35.3 0 64-28.7 64-64V128c0-35.3-28.7-64-64-64H64zm64 320H64V320c35.3 0 64 28.7 64 64zM64 192V128h64c0 35.3-28.7 64-64 64zM448 384c0-35.3 28.7-64 64-64v64H448zm64-192c-35.3 0-64-28.7-64-64h64v64zM288 160a96 96 0 1 1 0 192 96 96 0 1 1 0-192z" /></svg></span>

        </div>
    </div>
</div>
<div class="col-lg-4">
    <div class="white_card card_height_100 default_border_1px mb_20">
        <div class="white_card_header">
            <div class="box_header m-0">
                <div class="main-title">
                    <h3 class="m-0">Number Of Customers</h3>
                </div>

            </div>
        </div>
        <div class="white_card_body">
            <center>
                <h2 class="text-black centerd">@Model.Users.Count</h2>

            </center>
            <span class="float-right display-5 opacity-5"><i class="fa fa-users"></i></span>

        </div>
    </div>
</div>
<div class="col-lg-4">
    <div class="white_card card_height_100 default_border_1px mb_20">
        <div class="white_card_header">
            <div class="box_header m-0">
                <div class="main-title">
                    <h3 class="m-0">Number Of Testimonials</h3>
                </div>

            </div>
